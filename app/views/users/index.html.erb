<table class="table table-striped table-bordered">
    <thead>
        <td>id#</td>
        <td>Name</td>
        <td>Email</td>
    </thead>
    <% @users.each do |user|%>
    <tr>
        <td><%= user.id %></td>
        <td><%= link_to user.name, user %></td>
        <td><%= user.email %></td>
    </tr>
    <% end %>
</table>